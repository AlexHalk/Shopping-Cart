{% extends '::base.html.twig' %}

{% block body -%}
    <h1>UserCart</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
               {# <td>{{ entity.timestamp }}</td>  #}
            </tr>
            <tr>
                <th>Submitted</th>
                <td>{{ entity.submitted }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('usercart') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('usercart_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <a href = "{{ path('product_viewAdmin') }}">
            Admin View
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>


{% endblock %}
